{
  "name": "@serpi90/eslint-config",
  "version": "1.0.0",
  "description": "Custom ESLint preset",
  "type": "module",
  "scripts": {
    "lint": "eslint --fix *.js *.cjs",
    "format": "prettier --write *.js *.cjs"
  },
  "author": {
    "name": "Juli√°n Maestri"
  },
  "exports": {
    ".": {
      "import": "./eslint.config.js",
      "require": "./eslint.config.cjs"
    },
    "./prettier.config.js": {
      "import": "./prettier.config.js",
      "require": "./prettier.config.cjs"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "globals": "^13.24.0",
    "prettier": "^3.2.4"
  }
}